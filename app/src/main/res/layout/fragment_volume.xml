    <EditText
            android:layout_width="135dp"
            android:layout_height="47dp"
            android:inputType="numberDecimal|numberSigned"
            android:ems="10"
            android:id="@+id/pintInputText" android:layout_marginTop="4dp"
            app:layout_constraintTop_toBottomOf="@+id/textView16" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:hint="@string/pint"/>
    <EditText
            android:layout_width="135dp"
            android:layout_height="47dp"
            android:inputType="numberDecimal|numberSigned"
            android:ems="10"
            android:id="@+id/mlInputText" android:layout_marginTop="4dp"
