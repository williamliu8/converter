            android:layout_marginLeft="16dp"/>
    <EditText
            android:layout_width="135dp"
            android:layout_height="47dp"
            android:inputType="numberDecimal|numberSigned"
            android:ems="10"
            android:id="@+id/lInputText" android:layout_marginTop="4dp"
            app:layout_constraintTop_toBottomOf="@+id/textView20" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp" android:hint="@string/l"
            app:layout_constraintStart_toEndOf="@+id/gallonInputText" android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"/>
    <EditText
            android:layout_width="135dp"
            android:layout_height="47dp"
            android:inputType="numberDecimal|numberSigned"
            android:ems="10"
            android:id="@+id/quartInputText"
